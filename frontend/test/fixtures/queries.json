{
    "query1": {
        "WHERE": {
            "IS": {
                "courses_dept": "cpsc"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id"
            ],
            "ORDER": "courses_id"
        }
    },
    "coursesANDISComparator": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_instructor": "cox, barbara"
                    }
                },
                {
                    "IS": {
                        "courses_dept": "comm"
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_avg",
                "courses_instructor"
            ],
            "ORDER": "courses_avg"
        }
    },
    "coursesApplyAvg": {
        "WHERE": {
            "NOT": {
                "GT": {
                    "courses_avg": 95
                }
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_title",
                "overallAvg"
            ]
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_title"
            ],
            "APPLY": [
                {
                    "overallAvg": {
                        "AVG": "courses_avg"
                    }
                }
            ]
        }
    },
    "coursesConditions": {
        "WHERE": {
            "NOT": {
                "GT": {
                    "courses_avg": 95
                }
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_avg",
                "courses_instructor"
            ],
            "ORDER": "courses_avg"
        }
    },
    "coursesSortDown": {
        "WHERE": {
            "NOT": {
                "GT": {
                    "courses_avg": 95
                }
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_title",
                "courses_avg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "courses_dept"
                ]
            }
        }
    },
    "coursesTransformation2Apply": {
        "WHERE": {
            "NOT": {
                "GT": {
                    "courses_avg": 98.5
                }
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_title",
                "courses_id",
                "maxAverage",
                "minAverage"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "minAverage"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept",
                "courses_title",
                "courses_id"
            ],
            "APPLY": [
                {
                    "maxAverage": {
                        "MAX": "courses_avg"
                    }
                },
                {
                    "minAverage": {
                        "MIN": "courses_avg"
                    }
                }
            ]
        }
    },
    "roomsApplyCount": {
        "WHERE": {
            "GT": {
                "rooms_seats": 300
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "rooms_number",
                "countSeats"
            ]
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname",
                "rooms_number"
            ],
            "APPLY": [
                {
                    "countSeats": {
                        "COUNT": "rooms_seats"
                    }
                }
            ]
        }
    },
    "roomsComplex": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "maxSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
    "roomsSortUp": {
        "WHERE": {
            "GT": {
                "rooms_seats": 300
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "rooms_number"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "rooms_shortname",
                    "rooms_number"
                ]
            }
        }
    }
}
